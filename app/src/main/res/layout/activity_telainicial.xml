<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/plano7"
    tools:context=".Telainicial">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="378dp"
        android:layout_height="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.424"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.062"
        app:srcCompat="@drawable/minhasemana" />

    <ImageButton
        android:id="@+id/imageButton_segunda"
        android:layout_width="152dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.116"
        app:srcCompat="@drawable/segunda" />

    <ImageButton
        android:id="@+id/imageButton_terca"
        android:layout_width="152dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageButton_segunda"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageButton_segunda"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_segunda"
        app:layout_constraintVertical_bias="0.03"
        app:srcCompat="@drawable/terca" />

    <ImageButton
        android:id="@+id/imageButton_quarta"
        android:layout_width="152dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageButton_terca"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageButton_terca"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_terca"
        app:layout_constraintVertical_bias="0.03"
        app:srcCompat="@drawable/quarta" />

    <ImageButton
        android:id="@+id/imageButton_quinta"
        android:layout_width="152dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageButton_quarta"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageButton_quarta"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_quarta"
        app:layout_constraintVertical_bias="0.047"
        app:srcCompat="@drawable/quinta" />

    <ImageButton
        android:id="@+id/imageButton_sexta"
        android:layout_width="152dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageButton_quinta"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageButton_quinta"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_quinta"
        app:layout_constraintVertical_bias="0.051"
        app:srcCompat="@drawable/sexta" />

    <ImageButton
        android:id="@+id/imageButton_sabado"
        android:layout_width="152dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageButton_sexta"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageButton_sexta"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_sexta"
        app:layout_constraintVertical_bias="0.071"
        app:srcCompat="@drawable/sabado" />

    <ImageButton
        android:id="@+id/imageButton_domingo"
        android:layout_width="152dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageButton_sabado"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageButton_sabado"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_sabado"
        app:layout_constraintVertical_bias="0.115"
        app:srcCompat="@drawable/domingo" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="204dp"
        android:layout_height="191dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.922"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.152" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="207dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.921"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.48"
        app:srcCompat="@drawable/anotacoes" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="209dp"
        android:layout_height="268dp"
        android:backgroundTint="@color/lightcoral"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Escreva aqui"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.945"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.954" />

</androidx.constraintlayout.widget.ConstraintLayout>